  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch go backend",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/backend/main.go",
      "envFile": "${workspaceFolder}/backend/.env"
    },
    {
      "name": "Launch seed",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/backend/cmd/seed-run/main.go",
      "envFile": "${workspaceFolder}/backend/.env",
      "cwd": "${workspaceFolder}/backend"
    }
  ]
}
